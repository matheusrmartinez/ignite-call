{
  "name": "ignite-call",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"./**/*.{js,ts,tsx}\"",
    "format-check": "prettier --check \"./**/*.{js,ts,tsx,jsx}\"",
    "type-check": "tsc --pretty --noEmit --incremental false",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "prepare": "husky install",
    "pretty-quick": "npx -p prettier@latest -p pretty-quick pretty-quick --staged",
    "prettier-check": "prettier-check src/**/*.ts"
  },
  "lint-staged": {
    "*.(js|ts|jsx|tsx)": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{js,ts,tsx}": "prettier --write"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.0.1",
    "@ignite-ui/react": "^2.2.4",
    "@prisma/client": "^4.12.0",
    "@tanstack/react-query": "^4.29.3",
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "axios": "^1.3.5",
    "dayjs": "^1.11.7",
    "eslint": "8.37.0",
    "eslint-config-next": "13.2.4",
    "next": "13.2.4",
    "next-auth": "^4.21.1",
    "nookies": "^2.5.2",
    "phosphor-react": "^1.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "typescript": "5.0.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^1.2.0",
    "@types/cookie": "^0.5.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8",
    "prisma": "^4.12.0"
  }
}
